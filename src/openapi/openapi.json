          "division",
          "endDateOfAgreement",
          "fullName",
          "startDateOfAgreement"
        ],
        "type": "object",
        "properties": {
          "fullName": { "type": "string" },
          "acronym": { "type": "string" },
          "agreementNumber": { "type": "string" },
          "startDateOfAgreement": { "type": "string", "format": "date" },
          "endDateOfAgreement": { "type": "string", "format": "date" },
          "division": { "type": "string" },
          "centerPresentCandidateForDqp": { "type": "boolean" },
          "centerPresentCandidateForCqp": { "type": "boolean" }
        }
      },
      "CreateStaffRequest": {
        "required": [
          "dateOfBirth",
          "email",
          "firstname",
          "lastname",
          "nationalIdNumber",
          "phoneNumber",
          "positionName"
        ],
        "type": "object",
        "properties": {
          "firstname": { "type": "string" },
          "lastname": { "type": "string" },
          "email": { "type": "string" },
          "phoneNumber": {
            "maxLength": 2147483647,
            "minLength": 9,
            "type": "string"
          },
          "nationalIdNumber": {
            "maxLength": 2147483647,
            "minLength": 9,
            "type": "string"
          },
          "positionName": { "type": "string" },
          "dateOfBirth": { "type": "string" }
        }
      },
      "Application": {
        "type": "object",
        "properties": {
          "id": { "type": "integer", "format": "int64" },
          "createdDate": { "type": "string", "format": "date-time" },
          "lastModifiedDate": { "type": "string", "format": "date-time" },
          "createdBy": { "type": "integer", "format": "int64" },
          "lastModifiedBy": { "type": "integer", "format": "int64" },
          "startAcademicYear": { "type": "string", "format": "date" },
          "endAcademicYear": { "type": "string", "format": "date" },
          "candidate": { "$ref": "#/components/schemas/Candidate" },
          "speciality": { "$ref": "#/components/schemas/Speciality" },
          "archived": { "type": "boolean" },
          "actived": { "type": "boolean" }
        }
      },
      "Campus": {
        "type": "object",
        "properties": {
          "id": { "type": "integer", "format": "int64" },
          "createdDate": { "type": "string", "format": "date-time" },
          "lastModifiedDate": { "type": "string", "format": "date-time" },
          "createdBy": { "type": "integer", "format": "int64" },
          "lastModifiedBy": { "type": "integer", "format": "int64" },
          "name": { "type": "string" },
          "capacity": { "type": "integer", "format": "int32" },
          "quarter": { "type": "string" },
          "town": { "type": "string" },
          "trainingCenter": { "$ref": "#/components/schemas/TrainingCenter" },
          "ycoor": { "type": "number", "format": "double" },
          "xcoor": { "type": "number", "format": "double" },
          "archived": { "type": "boolean" },
          "actived": { "type": "boolean" }
        }
      },
      "Candidate": {
        "type": "object",
        "properties": {
          "idUser": { "type": "integer", "format": "int64" },
          "firstname": { "type": "string" },
          "lastname": { "type": "string" },
          "dateOfBirth": { "type": "string", "format": "date" },
          "email": { "type": "string" },
          "phoneNumber": { "type": "string" },
          "nationalIdNumber": { "type": "string" },
          "password": { "type": "string" },
          "accountLocked": { "type": "boolean" },
          "enabled": { "type": "boolean" },
          "roles": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/Role" }
          },
          "createdDate": { "type": "string", "format": "date-time" },
          "lastModifiedDate": { "type": "string", "format": "date-time" },
          "createdBy": { "type": "integer", "format": "int64" },
          "lastModifiedBy": { "type": "integer", "format": "int64" },
          "sex": { "type": "string" },
          "placeOfBirth": { "type": "string" },
          "motherFullName": { "type": "string" },
          "fatherFullName": { "type": "string" },
          "motherProfession": { "type": "string" },
          "fatherProfession": { "type": "string" },
          "highestSchoolLevel": { "type": "string" },
